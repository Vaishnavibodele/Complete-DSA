class Main {
    public static void main(String[] args) {
        int arr[] = {1, 4, 5, 7, 9};
        int target = 5;
        boolean found = false;
        for(int i = 0; i <= arr.length-1; i++){
            if(arr[i] == target){
                found = true;
                break;
            }    
        }    
            if(found){
                System.out.println("Element Found");
            }else{
                System.out.println("Element not Found");
            }
    }
}
