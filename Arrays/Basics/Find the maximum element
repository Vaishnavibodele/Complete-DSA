class Main {
    public static void main(String[] args) {
        int arr[] = {1, 5, 5, 7, 9};
        int max = 0;
        for (int i = 0; i <= arr.length-1; i++){
            if(max < arr[i]){
                max = arr[i];
            }
        }
        System.out.println("The maximum element is =" +" "+ max);
    }
}
